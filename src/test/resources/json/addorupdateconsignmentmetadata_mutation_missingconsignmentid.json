{
  "query": "mutation addOrUpdateConsignmentMetadata($addOrUpdateConsignmentMetadataInput: AddOrUpdateConsignmentMetadataInput!) {addOrUpdateConsignmentMetadata(addOrUpdateConsignmentMetadataInput:$addOrUpdateConsignmentMetadataInput) {consignmentId,propertyName,value}}",
  "variables": {
